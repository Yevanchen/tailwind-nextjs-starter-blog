{
  "name": "tailwind-nextjs-starter-blog",
  "version": "2.2.0",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "start": "next dev",
    "dev": "cross-env INIT_CWD=$PWD next dev",
    "build": "cross-env NODE_ENV=production NEXT_TELEMETRY_DISABLED=1 INIT_CWD=$PWD NODE_OPTIONS=\"--experimental-json-modules --experimental-vm-modules\" next build",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir app --dir components --dir lib --dir layouts --dir scripts",
    "prepare": "husky"
  },
  "dependencies": {
