    "@bytemd/plugin-medium-zoom": "^1.22.0",
    "@bytemd/plugin-mermaid": "^1.22.0",
    "@bytemd/react": "^1.22.0",
    "@headlessui/react": "^1.7.19",
    "@next/bundle-analyzer": "14.2.3",
    "@remirror/pm": "^3.0.0",
