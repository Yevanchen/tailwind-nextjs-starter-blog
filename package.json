    "hast-util-from-html-isomorphic": "^2.0.0",
    "image-size": "1.0.0",
    "jsonwebtoken": "^9.0.2",
    "katex": "^0.16.21",
    "nanoid": "^5.1.2",
    "next": "14.2.3",
    "next-contentlayer": "^0.3.4",
    "next-contentlayer2": "latest",
    "next-mdx-remote": "4.4.1",
